<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>PProvost.DevTools</id>
    <title>Peter Provost's Windows Dev Tools All-in-one Package</title>
    <version>2013.09.09</version>
    <authors>Peter Provost</authors>
    <owners>Peter Provost</owners>
    <summary>My set of utilities and tools for a new development machine.</summary>
    <description>These are the basic utilities and tools that I install on a new development machine or VM.
		
			This package was inspired by and modeled on Alan Stevens' Windows Utilities package (http://chocolatey.org/packages/alanstevens.utils)

			This package only contains dependencies to other packages. Please review each dependency to understand the license and usage instructions.
		</description>
    <projectUrl>https://github.com/PProvost/ChocolateyPackages</projectUrl>
    <tags>utilities tools</tags>
    <dependencies>
      <dependency id="curl" />
      <dependency id="fiddler" />
      <dependency id="git.install" />
      <dependency id="ilspy" />
      <dependency id="kickassvim" />
      <dependency id="p4merge" />
      <dependency id="git-credential-winstore" />
    </dependencies>
  </metadata>
</package>